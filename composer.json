{
    "name": "calcinai/php-amqplib-bridge",
    "type": "library",
    "description": "Bridge for videlalvaro/php-amqplib to expose classes compatible with PECL::AMQP",
    "homepage": "https://github.com/calcinai/php-amqplib-bridge/",
    "license": "MIT",

    "authors": [
        {
            "name": "Michael Calcinai",
            "email": "michael@calcin.ai"
        }
    ],

    "require": {
        "php-amqplib/php-amqplib": "^2.5"
    },

    "autoload": {
        "psr-0": {
            "AMQP": "src/"
        },
        "files": [
            "src/Constants.php"
        ]
    }

}
